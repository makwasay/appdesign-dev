<%= simple_form_for(@assignment) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :store_id %>
    <%= f.input :employee_id %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :pay_level %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
